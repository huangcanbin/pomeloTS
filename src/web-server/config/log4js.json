{
  "appenders": [
    {
      "type": "console"
    },
    {
      "type": "file",
      "filename": "logs/web-api.log",
      "maxLogSize": 20480,
      "backups": 3,
      "category": "api-logger"
    }
  ],
  "levels": {
    "api-logger" : "DEBUG"
  },

  "replaceConsole": true,

  "lineDebug": false
}